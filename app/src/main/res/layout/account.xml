				android:textSize="25sp"
				android:textStyle="bold"
				android:textColor="#E0E0E0" />
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:layout_marginLeft="15dp"
