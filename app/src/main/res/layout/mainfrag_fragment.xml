<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:overScrollMode="never">
		<LinearLayout
			android:id="@+id/linear1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#232B2E"
				android:orientation="horizontal"
				android:weightSum="1">
				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="50dp"
					android:layout_marginBottom="9dp"
					android:padding="8dp"
					android:text="Explore a Tonic"
					android:textSize="20sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0"
					android:translationY="10.0dp"
					android:layout_gravity="center_vertical" />
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="10dp"
					android:padding="8dp"
					android:gravity="right"
					android:orientation="horizontal"
					android:translationY="10.0dp"
					android:layout_gravity="center_vertical">
					<EditText
						android:id="@+id/edittext1"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:padding="8dp"
						android:background="@android:color/transparent"
						android:textSize="20sp"
						android:textColor="#E0E0E0"
						android:hint="Search"
						android:textColorHint="#E0E0E0"
						android:lines="1"
						android:imeOptions="actionSearch"
						android:layout_weight="1"
						
						android:maxLines="1"
						android:singleLine="true"
						 />
					<ImageView
						android:id="@+id/imageview2"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:src="@drawable/magnify"
						android:scaleType="centerInside"
						android:scaleX="0.8"
						android:scaleY="0.8"
						android:layout_gravity="right" />
					<ImageView
						android:id="@+id/imageview1"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:src="@drawable/bell_1"
						android:scaleType="centerInside"
						android:scaleX="0.8"
						android:scaleY="0.8"
						android:layout_gravity="right" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear44"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_margin="15dp"
				android:padding="8dp"
				android:gravity="right"
				android:orientation="horizontal"
				android:layout_weight="1"
				
				
				android:elevation="8dp"
				>
				<EditText
					android:id="@+id/edittext2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="@android:color/transparent"
					android:textSize="16sp"
					android:textColor="#E0E0E0"
					android:hint="Search"
					android:textColorHint="#E0E0E0"
					android:lines="1"
					android:imeOptions="actionSearch"
					android:layout_weight="1"
					
					android:maxLines="1"
					android:singleLine="true"
					 />
				<ImageView
					android:id="@+id/imageview11"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/close"
					android:scaleType="center"
					android:layout_gravity="center_vertical" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear7"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="50dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagetrend"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Trending"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear8"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagelove"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/hug"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="You may love"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagescience"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend_7"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Science "
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagemotive"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/fist"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Motivational"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview8"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagecomedy"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend_10"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Top comedy"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview7"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear28"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageliked"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend_4"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Most liked"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview6"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear30"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageseeding"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/seeding7"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview13"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="New"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear32"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageease"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend_13"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview14"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Feel ease"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear38"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imagerel"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="18dp"
					android:layout_marginTop="6dp"
					android:src="@drawable/trend_15"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview15"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="5dp"
					android:padding="8dp"
					android:text="Religions"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
			</LinearLayout>
			<androidx.recyclerview.widget.RecyclerView
				android:id="@+id/recyclerview3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp" />
			<LinearLayout
				android:id="@+id/linear12"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="50dp"
				android:layout_marginBottom="50dp"
				android:padding="8dp"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:padding="8dp"
					android:text="Explore Pod Tonic"
					android:textSize="18sp"
					android:textStyle="bold"
					android:textColor="#E0E0E0" />
				<LinearLayout
					android:id="@+id/linear40"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="4dp"
					android:padding="8dp"
					android:gravity="right"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="10dp"
						android:src="@drawable/ic_chevron_right_grey"
						android:scaleType="center"
						android:scaleX="1.75"
						android:scaleY="1.75" />
				</LinearLayout>
			</LinearLayout>
			<HorizontalScrollView
				android:id="@+id/hscroll11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content">
				<LinearLayout
					android:id="@+id/linear37"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="horizontal" />
			</HorizontalScrollView>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
